{
    "name": "anthonyabikhattar/laravel-phone",
    "description": "Adds phone number functionality to Laravel based on Google's libphonenumber API.",
    "keywords": [
        "laravel",
        "libphonenumber",
        "validation",
        "phone"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Propaganistas",
            "email": "Propaganistas@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/contracts": "^8.9|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/validation": "^8.0|^9.0",
        "anthonyabikhattar/libphonenumber-for-php": "dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "^9.5.10",
        "nunomaduro/larastan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Propaganistas\\LaravelPhone\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Propaganistas\\LaravelPhone\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Propaganistas\\LaravelPhone\\PhoneServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
